# Generated by supabase init
# Example configuration showing how to change ports

[api]
enabled = true
# Port for PostgREST - REST API
port = 54321
schemas = ["public", "graphql_public"]
extra_search_path = ["public", "extensions"]
max_rows = 1000

[db]
# Port for PostgreSQL database
# Change this if you have conflicts with existing PostgreSQL
port = 54322  # Default is 54322, change to 54323 or other if needed

[realtime]
enabled = true
# Port for Realtime WebSocket server
port = 54324

[studio]
enabled = true
# Port for Supabase Studio (web UI)
port = 54323

[inbucket]
enabled = true
# Port for email testing interface
port = 54324
# SMTP port for capturing emails
smtp_port = 54325
# POP3 port for email retrieval
pop3_port = 54326

[storage]
enabled = true
# Port for file storage service
port = 54327

[auth]
enabled = true
# Port for authentication service
port = 54328

# If you need to change ports due to conflicts:
# 1. Edit this file (rename to config.toml)
# 2. Change the port numbers as needed
# 3. Run: supabase stop
# 4. Run: supabase start
# 5. Update your .envrc with new connection strings